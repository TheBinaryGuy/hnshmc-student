{
    "cli": {
        "version": ">= 10.2.4"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "channel": "development",
            "android": {
                "buildType": "apk",
                "image": "latest"
            },
            "env": {
                "EXPO_PUBLIC_API_URL": "https://hnshmc-web.vercel.app"
            }
        },
        "preview": {
            "distribution": "internal",
            "channel": "preview",
            "android": {
                "buildType": "apk",
                "image": "latest"
            },
            "env": {
                "EXPO_PUBLIC_API_URL": "https://hnshmc-web.vercel.app"
            }
        },
        "production": {
            "channel": "production",
            "env": {
                "EXPO_PUBLIC_API_URL": "https://hnshmc-web.vercel.app"
            },
            "android": {
                "image": "latest"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
